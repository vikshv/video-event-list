{
    "name": "video-event-list",
    "private": true,
    "version": "1.0.0",
    "browserslist": [
        "last 2 versions"
    ],
    "jest": {
        "moduleNameMapper": {
            "^app(.*)$": "<rootDir>/src/app/$1",
            "^core(.*)$": "<rootDir>/src/core/$1"
        }
    },
    "scripts": {
        "start": "npm run web:hot",
        "static:install": "npm install --no-save",
        "web:hot": "npm run static:install && npm run web:hot:webpack",
        "web:hot:webpack": "node ./server.js",
        "web:build": "npm run static:install && npm run web:build:webpack",
        "web:build:webpack": "cross-env NODE_ENV=production webpack --config webpack.web.prod.config.js",
        "eslint": "gulp eslint",
        "tslint": "gulp tslint",
        "stylelint": "gulp stylelint",
        "test": "jest",
        "test:watch": "npm test -- --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/preset-env": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@types/jest": "^26.0.15",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/resize-observer-browser": "^0.1.4",
        "@types/webpack-env": "^1.15.3",
        "@typescript-eslint/eslint-plugin": "^4.5.0",
        "@typescript-eslint/parser": "^4.5.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^3.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.0",
        "css-minimizer-webpack-plugin": "^1.1.5",
        "eslint": "^7.12.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-react": "^7.21.5",
        "fork-ts-checker-webpack-plugin": "^6.0.1",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^26.6.3",
        "mini-css-extract-plugin": "^1.2.0",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "progress-bar-webpack-plugin": "^2.1.0",
        "stylelint": "^13.7.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-webpack-plugin": "^2.1.1",
        "ts-loader": "^8.0.6",
        "typescript": "^4.0.5",
        "webpack": "^4.44.2",
        "webpack-cli": "^4.1.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-notifier": "^1.8.0"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "@hot-loader/react-dom": "^17.0.0",
        "@reduxjs/toolkit": "^1.4.0",
        "axios": "^0.21.0",
        "dayjs": "^1.9.4",
        "normalize.css": "^8.0.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.1",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0"
    }
}
