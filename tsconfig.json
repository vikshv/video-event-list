{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
        "app/*": [
          "src/app/*"
        ],
        "core/*": [
          "src/core/*"
        ]
    },
    "outDir": "./dist/",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true
  },
  "include": [
    "src"
  ]
}
